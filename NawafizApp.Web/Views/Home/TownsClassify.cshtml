
@{
    ViewBag.Title = "TownsClassify";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>TownsClassify</h2>
<style>
    .dropbtn {
        background-color: #36c6d3;
        color: white;
        padding: 7px 5px;
        font-size: 16px;
        border: none;
        cursor: pointer;
        text-decoration: none !important;
    }
</style>
<div style="margin-right:235px;background-color:white;text-align:center;">
    <h2>لا تستطيع حذف هذه المنطقة  </h2><br />
    <h3>توجد إعلانات مرتبطة بها</h3>
    <div>
        <br /><br />
        @Html.ActionLink("العودة الى المحافظات", "Cities", "Home", new { @class = "dropbtn" })
        @Html.ActionLink("تفاصيل المنطقة", "EditTown", "Home", new { @townName = ViewBag.townName, @id = ViewBag.Id ,@cityID = ViewBag.cityId}, new { @class = "dropbtn", @style = "text-decoration: none;" })
    </div>
</div>
